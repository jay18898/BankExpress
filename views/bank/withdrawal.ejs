<style>
    body {font-family: Arial, Helvetica, sans-serif;}
    div{
        margin: 5px;
        padding:5px
    }
    
    input[type=radio] {
      width: 1%;
      padding: 12px 20px;
      margin: 8px 0;
      display: inline-block;
    
      box-sizing: border-box;
    }

    input{
      width: 25%;
      padding: 12px 20px;
      margin: 8px 0;
      display: inline-block;
    
      box-sizing: border-box;
    }
    button {
      
      color: white;
      padding: 14px 20px;
      margin: 8px 0;
      border: none;
      cursor: pointer;
      width: 100%;
    }
    
    button:hover {
      opacity: 0.8;
    }
    
    </style>
</head>
<body>
        <header>
                <% include ../partials/header %>
            </header>
	<div class="main-w3layouts wrapper">
	
			<div class="agileits-top">
				<form action="/insert_passbook" method="post">
                    <label for="accno">Enter Account No.:</label><br>
                    <input type="number" name="accno" placeholder="Account No" required="Please enter Account No."><br>
                    <label for="dpamount">Enter Deposit Amount:</label><br>
                    <input type="number" name="wdamount" placeholder="Withdrawal Amount" required="please enter withdrawal amount"><br>
                    <input type="submit" value="Withdrawal">
                    <input type="hidden"  name="form" value="wdform">
				</form>
			</div>
            <p><% if(msg) %><%=msg %></p>
	</div>
	
</body>
</html>